package com.flow.coretime.controller;

import org.springframework.stereotype.Controller;

@Controller
public class LoginController {
	@GetMapping("/login")
    public String login() {
        return "login"; // login.jsp 반환
    }

    @GetMapping("/")
    public String home() {
        return "home"; // 로그인 성공 후 이동할 페이지
    }
}
